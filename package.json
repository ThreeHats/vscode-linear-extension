{
  "name": "linear",
  "displayName": "Linear",
  "description": "Manage your Linear projects from VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:linear.connect",
    "onCommand:linear.getMyIssues",
    "onCommand:linear.addContextIssueComment"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "linear.connect",
        "title": "Linear: Connect"
      },
      {
        "command": "linear.getMyIssues",
        "title": "Linear: Get all my issues"
      },
      {
        "command": "linear.addContextIssueComment",
        "title": "Linear: Comment context issue"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/mocha": "8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "1.60.0",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "eslint": "7.27.0",
    "glob": "7.1.7",
    "mocha": "8.4.0",
    "typescript": "4.3.2",
    "vscode-test": "1.5.2"
  },
  "dependencies": {
    "@linear/sdk": "^1.19.0"
  }
}
